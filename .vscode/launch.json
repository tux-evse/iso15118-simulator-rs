{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "iso-2",
            "type": "lldb",
            "preLaunchTask": "binding-native-build",
            "request": "launch",
            "program": "afb-binder",
            "env": {
                "LD_LIBRARY_PATH": "/usr/local/lib64",
                "IFACE_SIMU": "simu-veth",
                "PKI_TLS_DIR": "../iso15118-binding-rs/afb-test/certs"
            },
            "args": [
                "-v",
                "--monitoring",
                "--trap-faults=0",
                "--config=binding-15118/etc/binding-iso2.yaml",
            ],
            "cwd": "${workspaceFolder}",
            "initCommands": [
                "target stop-hook add -n _start -o continue",
                "settings set target.disable-aslr false"
            ]
        },
        {
            "name": "pcap-tls-1.3",
            "type": "lldb",
            "preLaunchTask": "binding-native-build",
            "request": "launch",
            "program": "${env:HOME}/.cargo/build/debug/pcap-iso15118",
            "env": {
                "LD_LIBRARY_PATH": "/usr/local/lib64:/usr/local/lib",
            },
            "args": [
                "--pcap_in=../iso15118-binding-rs/afb-test/trace-logs/hello3-tls-1.3.pcapng",
                "--psklog_in=../iso15118-binding-rs/afb-test/trace-logs/hello3-tls-1.3.keylog",
                "--json_out=/tmp/iso15118-scenario.json",
                "--verbose=1",
            ],
            "cwd": "${workspaceFolder}",
            "initCommands": [
                "target stop-hook add -n _start -o continue",
                "settings set target.disable-aslr false"
            ]
        },
        {
            "name": "pcap-din-abb",
            "type": "lldb",
            "preLaunchTask": "binding-native-build",
            "request": "launch",
            "program": "${env:HOME}/.cargo/build/debug/pcap-iso15118",
            "env": {
                "LD_LIBRARY_PATH": "/usr/local/lib64:/usr/local/lib",
            },
            "args": [
                "--pcap_in=./afb-test/trace-logs/abb-normal-din.pcap",
                "--json_out=/tmp/iso15118-scenario.json",
                "--verbose=0",
            ],
            "cwd": "${workspaceFolder}",
            "initCommands": [
                "target stop-hook add -n _start -o continue",
                "settings set target.disable-aslr false"
            ]
        },
        {
            "name": "pcap-iso2-ionic5",
            "type": "lldb",
            "preLaunchTask": "binding-native-build",
            "request": "launch",
            "program": "${env:HOME}/.cargo/build/debug/pcap-iso15118",
            "env": {
                "LD_LIBRARY_PATH": "/usr/local/lib64:/usr/local/lib",
            },
            "args": [
                "--pcap_in=./afb-test/trace-logs/ionic5-iso2.pcap",
                "--json_out=/tmp/iso15118-scenario.json",
                "--verbose=0",
            ],
            "cwd": "${workspaceFolder}",
            "initCommands": [
                "target stop-hook add -n _start -o continue",
                "settings set target.disable-aslr false"
            ]
        },
        {
            "name": "pcap-iso2-audi-dc",
            "type": "lldb",
            "preLaunchTask": "binding-native-build",
            "request": "launch",
            "program": "${env:HOME}/.cargo/build/debug/pcap-iso15118",
            "env": {
                "LD_LIBRARY_PATH": "/usr/local/lib64:/usr/local/lib",
            },
            "args": [
                "--pcap_in=./afb-test/trace-logs/audi-dc-iso2.pcap",
